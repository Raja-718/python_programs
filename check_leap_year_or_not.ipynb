   ],
   "source": [
    "year = int(input(\"Enter a year:\"))\n",
    "if (year % 400) and (year % 100) == 0:\n",
    "    print(\"{0} is leap year\".format(year))\n",
    "elif (year % 4 ==0) and (year % 100 != 0):\n",
    "    print(\"{0} is leap year\".format(year))\n",
    "else:\n",
    "    print(\"Entered year is not leap year\")"
   ]
